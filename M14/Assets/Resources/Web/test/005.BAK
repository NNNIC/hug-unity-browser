<h1> FUNCTION #1 <h1>
<h2> スクリプト </h2>
<pre>
  function a()
  {
     document.write("called a");
  }
  function b()
  {
     document.write("called b");
  }
  function c()
  {
     document.write("called c");
  }
  a();
  b();
  c();
</pre>

<h2> 実行 </h2>
<p>
<script><!--
  function a()
  {
     document.write("called a, ");
  }
  function b()
  {
     document.write("called b, ");
  }
  function c()
  {
     document.write("called c, ");
  }
  a();
  b();
  c();
--></script>
</p>

<h1> FUNCTION #2<h1>
<h2> スクリプト </h2>
<pre>
  function a(x)
  {
     document.write("x="+x+", ");
  }
  a("1");
  a("2");
  a("3");  
</pre>

<h2> 実行 </h2>
<p>
<script><!--
  function a(x)
  {
     document.write("x="+x+", ");
  }
  a("1");
  a("2");
  a("3");  
--></script>
</p>

<h1> FUNCTION #3<h1>
<h2> スクリプト </h2>
<pre>
  function a(x,y)
  {
     document.write("(x,y)=("+ x +"," + y +"), "  );
  }
  a(1,2);
  a("a","b");
  a("x","y");  
</pre>

<h2> 実行 </h2>
<p>
<script><!--
  function a(x,y)
  {
     document.write("(x,y)=("+ x +"," + y +"), "  );
  }
  a(1,2);
  a("a","b");
  a("x","y");  
--></script>
</p>

