<h1> TEST 3 </h1>
<table>
<tr>

<td>
<pre>
  a = 1;
  document.write(a++);               
  document.write(a);               
  document.write(++a);               
  document.write(a);               
  b = 100;
  document.write(b--);             
  document.write(b);               
  document.write(--b);             
  document.write(b);               
</pre>
</td>

<td>
<script><!--
   a = 1;                              document.write("<br>");
   document.write(a++);                document.write("<br>"); 
   document.write(a);                  document.write("<br>"); 
   document.write(++a);                document.write("<br>");
   document.write(a);                  document.write("<br>");
   b = 100;                            document.write("<br>");
   document.write(b--);                document.write("<br>");     
   document.write(b);                  document.write("<br>");
   document.write(--b);                document.write("<br>");
   document.write(b);                  
// --></script>
</td>
</tr>
</table>


