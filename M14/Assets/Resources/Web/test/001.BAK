<h1> TEST 2 </h1>
<table>
<tr>

<td>
<pre>
  document.write(12/2);               
  document.write(123/4);               
  document.write(123.4/2);             
  document.write(1*2*3);              
  document.write(1+2*3);            
  document.write(4+4/2);          
  document.write((2+2)*4);                 
  document.write(5%2);               
  document.write(-3*2);       
</pre>
</td>

<td>
<script><!--
  document.write(12/2);               document.write("<br>");
  document.write(123/4);              document.write("<br>"); 
  document.write(123.4/2);            document.write("<br>"); 
  document.write(1*2*3);              document.write("<br>");
  document.write(1+2*3);              document.write("<br>");
  document.write(4+4/2);              document.write("<br>");
  document.write((2+2)*4);            document.write("<br>");     
  document.write(5%2);                document.write("<br>");
  document.write(-3*2);       
